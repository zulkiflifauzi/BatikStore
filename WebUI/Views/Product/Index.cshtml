@model IEnumerable<WebUI.Models.ViewModelProduct>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Number
        </th>
        <th>
            Description
        </th>
        <th>
            Zone_ZoneId
        </th>
        <th>
            Model_ModelId
        </th>
        <th>
            Type_TypeId
        </th>
        <th>
            Origin_OriginId
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Zone_ZoneId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Model_ModelId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type_TypeId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Origin_OriginId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
